@font-face {
  font-family: 'flicons';
  src: url('<%= asset_path "flicons.eot?97805338" %>');
  src: url('<%= asset_path "flicons.eot?97805338" %>#iefix') format('embedded-opentype'),
       url('<%= asset_path "flicons.woff?97805338" %>') format('woff'),
       url('<%= asset_path "flicons.ttf?97805338" %>') format('truetype'),
       url('<%= asset_path "flicons.svg?97805338" %>#flicons') format('svg');
  font-weight: normal;
  font-style: normal;
}
/* Chrome hack: SVG is rendered more smooth in Windozze. 100% magic, uncomment if you need it. */
/* Note, that will break hinting! In other OS-es font will be not as sharp as it could be */
